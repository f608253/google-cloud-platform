##NodeJS index.js

const { GoogleAuth } = require('google-auth-library')
exports.invokeFnA = async (req, res) => {
  const url = https://us-central1-mindful-coder-367810.cloudfunctions.net/http-secured :
  const targetAudience = url;
  const auth = new GoogleAuth();

  try {
    const client = await auth.getIDTokenClient(targetAudience);
    const FnAResp = await client.request({ url });
    res.send ({ message: fnAResp.data })
  } catch (err) {
    console.error(error.message);
    res.send({ message: err.message })
  }
}
